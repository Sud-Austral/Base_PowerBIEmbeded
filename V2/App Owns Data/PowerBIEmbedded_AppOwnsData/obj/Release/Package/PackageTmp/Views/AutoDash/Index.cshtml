@model PowerBIEmbedded_AppOwnsData.Models.EmbedConfig

@{
    ViewBag.Title = @ViewBag.titulo;
    Layout = "~/Views/Shared/_Layout.cshtml";
    //Layout =null;
}
<style>
    body {
        background: url("~/Content/Image/dataintelligence.png") no-repeat center center fixed !important;
        background-color: black !important;
        background-size: cover !important;
        -moz-background-size: cover !important;
        -webkit-background-size: cover !important;
        -o-background-size: cover !important;
    }
</style>
<script src="https://npmcdn.com/es6-promise@3.2.1"></script>
<script src="~/scripts/powerbi.js"></script>

<div id="contenedor" style="background: url(&quot;../../Content/Image/dataintelligence.png&quot;) no-repeat center center fixed !important;
        background-color: black !important;
        /*background-size: cover !important;
        -moz-background-size: cover !important;
        -webkit-background-size: cover !important;
        -o-background-size: cover !important;   no-repeat  */">
    <iframe 
            
            style="width:100%;height:100%;" 
            src="@ViewBag.url" 
            scrolling="no" allowfullscreen="true">
    </iframe>
</div>

<style type="text/css">
    html, body, div, iframe {
        margin: 0;
        padding: 0;
        height: 100%;
    }

    iframe {
        display: block;
        width: 100%;
        border: none;
    }
</style>